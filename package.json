{
  "name": "@power-elements/stripe-elements",
  "version": "1.1.2",
  "description": "Web Component wrapper for stripe elements",
  "main": "stripe-elements.js",
  "module": "stripe-elements.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "run-p build:*",
    "build:rollup": "rollup -c",
    "build:analyze": "wca analyze src/StripeElements.js --outFile custom-elements.json",
    "test": "karma start --coverage",
    "test:watch": "karma start --auto-watch=true --single-run=false",
    "test:update-snapshots": "karma start --update-snapshots",
    "test:prune-snapshots": "karma start --prune-snapshots",
    "site:build": "npm run storybook:build",
    "storybook": "start-storybook --node-resolve --watch --open --babel",
    "storybook:build": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bennypowers/stripe-elements.git"
  },
  "files": [
    "*.js",
    "_virtual/*",
    "src/*",
    "demo/*",
    "README.md",
    ".babelrc",
    "index.html",
    "test/*"
  ],
  "keywords": [
    "web-components",
    "lit-element",
    "lit-element-2.0",
    "stripe",
    "payment"
  ],
  "author": "Benny Powers <web@bennypowers.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bennypowers/stripe-elements/issues"
  },
  "homepage": "https://github.com/bennypowers/stripe-elements#readme",
  "devDependencies": {
    "@apollo-elements/eslint-config": "^1.1.0",
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-proposal-private-methods": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "@lrnwebcomponents/json-editor": "^2.3.3",
    "@material/mwc-button": "^0.11.1",
    "@material/mwc-textfield": "^0.11.1",
    "@open-wc/demoing-storybook": "^1.3.4",
    "@open-wc/testing": "^2.4.2",
    "@open-wc/testing-karma": "^3.2.17",
    "@polymer/iron-demo-helpers": "^3.1.0",
    "@polymer/paper-button": "^3.0.1",
    "@polymer/paper-input": "^3.1.0",
    "@power-elements/json-viewer": "^1.0.1",
    "@storybook/addon-docs": "^5.2.8",
    "@webcomponents/webcomponentsjs": "^2.4.0",
    "babel-eslint": "^10.0.3",
    "bound-decorator": "^1.1.0",
    "chai-things": "^0.2.0",
    "crocks": "^0.12.4",
    "deepmerge": "^4.2.2",
    "eslint": "^6.7.2",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-json": "^2.0.1",
    "eslint-plugin-no-loops": "^0.3.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.27.13",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.3.0",
    "web-component-analyzer": "^1.0.0"
  },
  "dependencies": {
    "lit-element": "^2.2.1"
  }
}
