{
  "name": "@power-elements/stripe-elements",
  "version": "3.3.0",
  "description": "Web Component wrapper for stripe elements",
  "type": "module",
  "main": "index.js",
  "module": "index.js",
  "customElements": "custom-elements.json",
  "directories": {
    "test": "test"
  },
  "exports": {
    ".": {
      "esbuild": "./src/index.ts",
      "default": "./index.js"
    },
    "./*": {
      "esbuild": "./src/*.ts",
      "default": "./*.js"
    },
    "./*.js": {
      "esbuild": "./src/*.ts",
      "default": "./*.js"
    }
  },
  "scripts": {
    "postinstall": "patch-package",
    "prepare": "npm run build",
    "build": "run-p build:**",
    "build:types": "tsc --emitDeclarationOnly && cp src/*.d.ts . && cp src/lib/*.d.ts ./lib/",
    "build:esbuild": "node scripts/build.js",
    "build:analyze": "cem analyze",
    "docs": "eleventy --incremental",
    "start": "eleventy --incremental --watch --serve",
    "clean": "run-s clean:*",
    "clean:src": "rimraf '{src,test,lib}/**/!(global).{js,js.map,d.ts}'",
    "clean:index": "rimraf './!(*.config|web-*).{js,js.map,d.ts}'",
    "lint": "eslint src/**/*.ts",
    "test": "wtr --coverage",
    "test:watch": "wtr --watch",
    "update": "npm-check -u",
    "watch": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bennypowers/stripe-elements.git"
  },
  "files": [
    "*.d.ts",
    "*.js",
    "*.js.map",
    "lib/*",
    "custom-elements.json",
    "CHANGELOG.md",
    "README.md"
  ],
  "types": "index.d.ts",
  "keywords": [
    "web-components",
    "stripe",
    "payment",
    "payment-request",
    "payment request api"
  ],
  "author": "Benny Powers <web@bennypowers.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bennypowers/stripe-elements/issues"
  },
  "homepage": "https://bennypowers.dev/stripe-elements",
  "dependencies": {
    "@lavadrop/camel-case": "^0.4.0",
    "@lavadrop/kebab-case": "^3.0.0",
    "@pacote/memoize": "^1.1.8",
    "@stripe/stripe-js": "^1.46.0",
    "lit": "^2.6.1",
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0",
    "@11ty/eleventy-plugin-directory-output": "^1.0.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.2.0",
    "@custom-elements-manifest/analyzer": "^0.5.7",
    "@custom-elements-manifest/to-markdown": "^0.1.0",
    "@lit-labs/ssr": "^3.0.1",
    "@material/mwc-button": "^0.27.0",
    "@material/mwc-dialog": "^0.27.0",
    "@material/mwc-textfield": "^0.27.0",
    "@open-wc/testing": "^3.1.7",
    "@open-wc/testing-karma": "^4.0.9",
    "@orchidjs/eleventy-plugin-ids": "^0.1.1",
    "@power-elements/codesandbox-button": "^0.1.0",
    "@power-elements/json-viewer": "^2.1.1",
    "@pwrs/eslint-config": "^0.0.26",
    "@types/credit-card-type": "^9.0.0",
    "@types/express": "^4.17.17",
    "@types/express-serve-static-core": "^4.17.33",
    "@types/node": "^18.13.0",
    "@types/sinon": "^10.0.13",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@web/dev-server-esbuild": "^0.3.3",
    "@web/test-runner": "^0.15.0",
    "@web/test-runner-playwright": "^0.9.0",
    "cem-plugin-module-file-extensions": "^0.0.5",
    "credit-card-type": "^9.1.0",
    "eleventy-plugin-toc": "^1.1.5",
    "esbuild": "^0.17.7",
    "esbuild-plugin-lit-css": "^2.0.1",
    "eslint": "^8.34.0",
    "execa": "^7.0.0",
    "husky": "^8.0.3",
    "luhn-js": "^1.1.2",
    "markdown-it-anchor": "^8.6.6",
    "npm-check": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.5.1",
    "sinon": "^15.0.1",
    "stylelint": "^15.1.0",
    "stylelint-config-standard": "^30.0.1",
    "typescript": "^4.9.5",
    "typescript-lit-html-plugin": "^0.9.0",
    "web-dev-server-plugin-lit-css": "^2.0.2"
  }
}
