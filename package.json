{
  "name": "@power-elements/stripe-elements",
  "version": "1.0.1",
  "description": "Web Component wrapper for stripe elements",
  "main": "stripe-elements.js",
  "module": "stripe-elements.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "prepare": "rollup -c",
    "test": "karma start",
    "site:build": "npm run storybook:build",
    "storybook": "start-storybook -p 9001",
    "storybook:build": "build-storybook -o _site -s storybook-static"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bennypowers/stripe-elements.git"
  },
  "files": [
    "*.js",
    "_virtual/*",
    "src/*",
    "demo/*",
    "README.md",
    ".babelrc",
    "index.html",
    "test/*"
  ],
  "keywords": [
    "web-components",
    "lit-element",
    "lit-element-2.0",
    "stripe",
    "payment"
  ],
  "author": "Benny Powers <web@bennypowers.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bennypowers/stripe-elements/issues"
  },
  "homepage": "https://github.com/bennypowers/stripe-elements#readme",
  "devDependencies": {
    "@apollo-elements/eslint-config": "^1.0.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-proposal-private-methods": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.2",
    "@open-wc/demoing-storybook": "^0.2.0",
    "@open-wc/testing": "^0.10.1",
    "@open-wc/testing-karma": "^0.4.15",
    "@storybook/addon-knobs": "^5.0.5",
    "babel-eslint": "^10.0.1",
    "bound-decorator": "^1.1.0",
    "chai-things": "^0.2.0",
    "crocks": "^0.11.1",
    "eslint": "^5.15.3",
    "eslint-config-google": "^0.12.0",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "rollup": "^1.7.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-node-resolve": "^4.0.1",
    "sinon": "^7.3.0",
    "sinon-chai": "^3.3.0"
  },
  "dependencies": {
    "@webcomponents/webcomponentsjs": "^2.2.7",
    "lit-element": "^2.1.0"
